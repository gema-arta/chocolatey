    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <iconUrl>https://turbo.net/io/users/turbo/upload/Turbo.png</iconUrl>
    <releaseNotes>This package uses turbo-plugin.exe and installs the latest version of Turbo.Net Plugin, regardless of the version specified in the package.</releaseNotes>
  </metadata>
